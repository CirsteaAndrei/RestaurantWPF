<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Restaurant.ViewModels"
                    xmlns:view="clr-namespace:Restaurant.Views">

    <DataTemplate DataType="{x:Type vm:LoginViewModel }">
        <view:Login/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminNavigationVM}">
        <view:AdminDashboard/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:AdminManageUsersVM}">
        <view:AdminManageUsersView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminManageTablesVM}">
        <view:AdminManageTablesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminManageProductsVM}">
        <view:AdminManageProductsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminGenerateReportsVM}">
        <view:AdminGenerateReportsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WaiterDashboardVM}">
        <view:WaiterDashboard/>
    </DataTemplate>
    

</ResourceDictionary>